{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "interruptible": true,
      "interactive": true
    },
    "build": {
      "dependsOn": ["^build"],
      "cache": true,
      "outputs": ["dist"]
    }
  }
}
