{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-native",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "metro": ["../../external/metro/packages/metro/types"],
      "metro/src/*": ["../../external/metro/packages/metro/types/*"],
      "metro-*": ["../../external/metro/packages/metro-*/types"],
      "metro/src/DeltaBundler/Serializers/baseJSBundle": [
        "./vendor/metro/baseJSBundle"
      ],
      "metro/src/lib/bundleToString": ["./vendor/metro/bundleToString"]
    }
  }
}
